#辞書の連結
d1 = {'A': 111, 'B': 222, 'C': 333}
d2 = {'D': 444, 'E': 555} 
d3 = {'F': 666}
#for文を使わない辞書の連結1
newdic = {**d1,**d2,**d3}
#2パイプによる連結
newdic = d1|d2|d3
print(newdic)#{'A': 111, 'B': 222, 'C': 333, 'D': 444, 'E': 555, 'F': 666}
#3.update()による連結
d4 = {}
for d in (d1,d2,d3):
	#.update()は引数を一つしかもてない
d4.update(d)
print(d4))#{'A': 111, 'B': 222, 'C': 333, 'D': 444, 'E': 555, 'F': 666}
