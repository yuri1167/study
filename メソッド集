#リストのインデックスを取得
numlist = [0,'1',3,2,'4',5,'7']
#引数にリストの値を入れることにより取得
nuwL = [l for l in numlist if int(l)==numlist.index(l)]
print(nuwL)

#型の判定
#数字のみ取り出す二種類の方法
lists = [1,'3',4,'5',6,'7']
newL = [l for l in lists if isinstance(l,int)]
print(newL)

lists = [1,'3',4,'5',6,'7']
newL = [l for l in lists if type(l)==int]
print(newL)
